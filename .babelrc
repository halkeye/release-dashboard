{
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-object-rest-spread",
    "transform-runtime"
  ],
  "presets": [
    ["env", { "targets": { "node": "current" } }],
    ["es2015", { "modules": false }],
    "react"
  ],
  "env": {
    "development": {
      "presets": [
        "react-hmre"
      ]
    }
  }
}
